{
  "hosting": {
    "public": "source",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ 
      {
      "source": "/index.html",
      "destination": "/home-page.html"
      },
      {
      "source": "/",
      "destination": "/home-page.html"
      }
    ]
  }
}
